		"@supabase/supabase-js": "^2.39.1",
		"@sveltejs/adapter-cloudflare": "^4.1.0",
		"bits-ui": "^0.16.0",
		"clsx": "^2.1.0",
		"cmdk-sv": "^0.0.13",
		"formsnap": "^0.4.3",
		"sveltekit-superforms": "^1.13.4",
		"tailwind-merge": "^2.2.1",
		"tailwind-variants": "^0.1.20",
		"zod": "^3.22.4"
